<?xml version="1.0" encoding="UTF-8"?>
<XDF name="accrsh">
    <Port kind="Input" name="inStream0">
        <Type name="int">
            <Entry kind="Expr" name="size">
                <Expr kind="Literal" literal-kind="Integer" value="16"/>
            </Entry>
        </Type>
    </Port>
    <Port kind="Output" name="outStream0">
        <Type name="int">
            <Entry kind="Expr" name="size">
                <Expr kind="Literal" literal-kind="Integer" value="16"/>
            </Entry>
        </Type>
    </Port>
    <Instance id="acc">
        <Class name="accumulator"/>
    </Instance>
    <Instance id="rsh">
        <Class name="rshifter"/>
    </Instance>
    <Connection dst="acc" dst-port="in0" src="" src-port="inStream0"/>
    <Connection dst="rsh" dst-port="in0" src="acc" src-port="out0"/>
    <Connection dst="" dst-port="outStream0" src="rsh" src-port="out0"/>
</XDF>
